  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "setup-env": "node scripts/setup-env.js",
    "setup-database": "node scripts/setup-database.js",
    "verify-database": "node scripts/verify-database.js",
    "check-env": "node -e \"console.log('Environment check:'); console.log('CLERK:', !!process.env.VITE_CLERK_PUBLISHABLE_KEY); console.log('SUPABASE:', !!process.env.VITE_SUPABASE_URL); console.log('RAZORPAY:', !!process.env.VITE_RAZORPAY_KEY_ID); console.log('ZERODHA:', !!process.env.VITE_ZERODHA_API_KEY);\"",
    "mobile:add:ios": "npm run build && npx cap add ios",
    "mobile:add:android": "npm run build && npx cap add android",
    "mobile:build": "npm run build && npx cap sync",
    "mobile:open:ios": "npx cap open ios",
    "mobile:open:android": "npx cap open android",
    "mobile:run:ios": "npm run mobile:build && npx cap run ios",
    "mobile:run:android": "npm run mobile:build && npx cap run android",
    "mobile:sync": "npx cap sync",
    "mobile:update": "npx cap update"
  },
